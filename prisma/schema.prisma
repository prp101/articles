// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Article {
  id     String @id
  title  String
  slug  String
  published_at  DateTime? @db.Date
}

model User {
  id     String @id
  username  String @unique
  password String
}